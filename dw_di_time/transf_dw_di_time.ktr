    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var quarter1_of_year = 1; // 1nd Quarter 1

if(month_of_year > 2 &amp;&amp; month_of_year &lt;= 4) {
	quarter1_of_year = 2;
} 
else if(month_of_year > 4 &amp;&amp; month_of_year &lt;= 6) {
	quarter1_of_year = 3;
}
else if(month_of_year > 6 &amp;&amp; month_of_year &lt;= 8) {
	quarter1_of_year = 4;
}
else if(month_of_year > 8 &amp;&amp; month_of_year &lt;= 10) {
	quarter1_of_year = 5;
}
else if(month_of_year > 10 &amp;&amp; month_of_year &lt;= 12) {
	quarter1_of_year = 6;
}
</jsScript_script>
      </jsScript>
    </jsScripts>
