var is_national_holiday = 0;

if(month_of_year == 1 &amp;&amp; day_of_month == 1) {
	is_national_holiday = 1;
}
else if(month_of_year == 4 &amp;&amp; day_of_month == 21) {
	is_national_holiday = 1;
}
else if(month_of_year == 5 &amp;&amp; day_of_month == 1) {
	is_national_holiday = 1;
}
else if(month_of_year == 9 &amp;&amp; day_of_month == 7) {
	is_national_holiday = 1;
}
else if(month_of_year == 11 &amp;&amp; day_of_month == 2) {
	is_national_holiday = 1;
}
else if(month_of_year == 11 &amp;&amp; day_of_month == 15) {
	is_national_holiday = 1;
}
else if(month_of_year == 12 &amp;&amp; day_of_month == 25) {
	is_national_holiday = 1;
}
</jsScript_script>
